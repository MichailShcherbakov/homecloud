{
  "name": "homecloud",
  "productName": "homecloud",
  "version": "0.0.1",
  "description": "My Electron application description",
  "main": "dist/electron/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "yarn prebuild && tsc && vite build",
    "dev": "vite",
    "preview": "vite preview",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "fmt": "prettier --write \"src/**/*.ts\"",
    "fmt:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [],
  "author": {
    "name": "MichailShcherbakov",
    "email": "50011226+MichailShcherbakov@users.noreply.github.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-wix",
          "config": {
            "appDirectory": "./out/homecloud-win32-x64",
            "name": "homecloud",
            "exe": "homecloud",
            "version": "0.0.1",
            "language": 1033,
            "manufacturer": "HomeCloud Comp.",
            "appIconPath": "./public/icon.ico",
            "description": "Your personal home storage",
            "ui": {
              "chooseDirectory": false
            }
          }
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.67",
    "@electron-forge/maker-wix": "^6.0.0-beta.67",
    "@swc/core": "^1.3.6",
    "@types/express": "^4.17.14",
    "@types/fluent-ffmpeg": "^2.1.20",
    "@types/hidefile": "^3.0.0",
    "@types/ini": "^1.3.31",
    "@types/node-forge": "^1.3.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "electron": "^21.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "vite-plugin-node": "^1.0.0"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@nestjs/common": "^9.1.4",
    "@nestjs/core": "^9.1.4",
    "@nestjs/platform-express": "^9.1.4",
    "@nestjs/serve-static": "^3.0.0",
    "chokidar": "^3.5.3",
    "electron-squirrel-startup": "^1.0.0",
    "ffmpeg": "^0.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "hidefile": "^3.0.0",
    "ini": "^3.0.1",
    "node-forge": "^1.3.1",
    "queue": "^6.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.7",
    "uuid": "^9.0.0",
    "vite": "^3.1.7",
    "vite-plugin-electron": "^0.10.0"
  }
}
